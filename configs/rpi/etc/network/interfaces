auto wlan0
allow-hotplug wlan0
iface wlan0 inet static
	address 192.168.0.128
	netmask 255.255.255.0
	gateway 192.168.0.1
	dns-nameservers 1.1.1.1 8.8.4.4
	wpa-ssid milko_do
	wpa-psk taekwondo
